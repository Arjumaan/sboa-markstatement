<script language="javascript" type="text/javascript">
  funtion sub()
  {
    if(document.getElementById("group1.i").value=="group1.i")
      const combobox = document.querySelector('smart-combo-box'); combobox. dataSource = ["English", "Language","Physics","Chemistry","Biology","Mathematics"];
    if(document.getElementById("group2.i").value=="group2.i")
      const combobox = document.querySelector('smart-combo-box'); combobox. dataSource = ["English", "Language","Physics","Chemistry","Computer Science","Mathematics"];
    if(document.getElementById("group3.i").value=="group3.i")
      const combobox = document.querySelector('smart-combo-box'); combobox. dataSource = ["English", "Language","Accounts","Commerce","Economics","Computer Applications"];
    if(document.getElementById("group4.i").value=="group4.i")
      const combobox = document.querySelector('smart-combo-box'); combobox. dataSource = ["English", "Language","Accounts","Commerce","Economics","Business Maths"];
  }
</script>
