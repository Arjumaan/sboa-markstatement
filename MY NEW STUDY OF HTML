<script>
 //for adding new rows in the table....
 //in the tabel the  number of rows taken from the database is also created here
  function row()
  {
    var marktable =  document.getElementById("marktable");
    var rows = marktable.rows.length;
    var r = marktable.insertRow(rows);
/* var c1 will create an element & var name will make the input & the rest of the part like c1.appendChild
name and r.append will give the names and values and types to the row */
    var c1 = document.createElement("TD");
    var name = document.createElement("INPUT");
    c1.appendChild(name);
    r.append(c1);
    name.setAttribute('type','text');
    name.setAttribute('size','20');

    var c2 = document.createElement("TD");
    var adno = document.createElement("INPUT");
    c2.appendChild(adno);
    r.append(c2);
    adno.setAttribute('type','text');
    adno.setAttribute('size','10');

    var c3 = document.createElement("TD");
    var rollno = document.createElement("INPUT");
    c3.appendChild(rollno);
    r.append(c3);
    rollno.setAttribute('type','integer');
    rollno.setAttribute('size','5');

    var c4 = document.createElement("TD");
    var groups = document.createElement("INPUT");
    c4.appendChild(groups);
    r.append(c4);
    groups.setAttribute('type','text');
    groups.setAttribute('size','10');

    var c6 = document.createElement("TD");
    var exam = document.createElement("INPUT");
    c6.appendChild(exam);
    r.append(c6);
    exam.setAttribute('type','text');
    exam.setAttribute('size','20');

    var c7 = document.createElement("TD");
    var eng = document.createElement("INPUT");
    c7.appendChild(eng);
    r.append(c7);
    eng.setAttribute('type','integer');
    eng.setAttribute('size','20');

    var c8 = document.createElement("TD");
    var lang = document.createElement("INPUT");
    c8.appendChild(lang);
    r.append(c8);
    lang.setAttribute('type','integer');
    lang.setAttribute('size','20');

    var c9 = document.createElement("TD");
    var phy_eco = document.createElement("INPUT");
    c9.appendChild(phy_eco);
    r.append(c9);
    phy_eco.setAttribute('type','integer');
    phy_eco.setAttribute('size','20');

    var c10 = document.createElement("TD");
    var che_acc = document.createElement("INPUT");
    c10.appendChild(che_acc);
    r.append(c10);
    che_acc.setAttribute('type','integer');
    che_acc.setAttribute('size','20');

    var c11 = document.createElement("TD");
    var mat_com = document.createElement("INPUT");
    c11.appendChild(mat_com);
    r.append(c11);
    mat_com.setAttribute('type','integer');
    mat_com.setAttribute('size','20');

    var c12 = document.createElement("TD");
    var bio_cs_ca_bm = document.createElement("INPUT");
    c12.appendChild(bio_cs_ca_bm);
    r.append(c12);
    bio_cs_ca_bm.setAttribute('type','int');
    bio_cs_ca_bm.setAttribute('size','20');

    var c17 = document.createElement("TD");
    var total = document.createElement("INPUT");
    c17.appendChild(total);
    r.append(c17);
    total.setAttribute('type','int');
    total.setAttribute('size','20');

    var c18 = document.createElement("TD");
    var avg = document.createElement("INPUT");
    c18.appendChild(avg);
    r.append(c18);
    avg.setAttribute('type','text');
    avg.setAttribute('size','20');
    //this will increase the
    for(var i = rows + 1; i < 1; i++)
    {
        if(marktable.rows[i].cells[1].checked)
        {
            marktable.addRow(i);
        }
    }
  }
  //after pressing submit button the below function must takes place to open action.php
</script>
